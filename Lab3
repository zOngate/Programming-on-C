#include <stdio.h>
#include <math.h>
    
int main()
{
	
    double a,b,c;
    scanf("%lf", &a);
    scanf("%lf", &b);
    scanf("%lf", &c);
    double x1,x2,d = 0;
    d = pow(b,2) - 4*a*c;
    printf("d = %lf \n", d);
    if (d>0){
    	x1 = (-b+sqrt(d))/(2*a);
    	x2 = (-b-sqrt(d))/(2*a);
    	printf("x1 = %lf x2 = %lf",x1,x2);
	}
	if (d<0){
		printf("Korney net");
	}
    if (d==0){
    	printf("Koren 1 \n");	
    	x1 = (-b+sqrt(d))/(2*a);
    	printf("x = %lf", x1);
	}
	]  
  return 0;
}
